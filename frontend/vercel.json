{
  "version": 2,
  "builds": [
    {
      "src": "backend/ecommerce_project/wsgi.py",
      "use": "@vercel/python",
      "config": { "runtime": "python3.9" }
    },
    {
      "src": "backend/build.sh",
      "use": "@vercel/static-build",
      "config": {
      "distDir": "staticfiles_build"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|svg|ico|json|woff2?|ttf|eot|otf|map))",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "backend/ecommerce_project/wsgi.py"
    }
  ]
}
